{
    "name": "hubleto/erp-project",
    "version": "0.25",
    "license": "AGPL-3.0-or-later",
    "description": "Production-ready Hubleto ERP.",
    "homepage": "https://www.hubleto.com",
    "support": {
        "issues": "https://github.com/hubleto/erp/issues",
        "source": "https://github.com/hubleto/erp"
    },
    "keywords": ["hubleto", "crm", "erp", "application", "hub"],
    "require": {
        "hubleto/framework": "@dev",
        "hubleto/erp": "@dev",
        "hubleto/assets": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Hubleto\\App\\Custom\\": "src/apps",
            "HubletoProject\\Hook\\": "src/hooks",
            "HubletoProject\\Cron\\": "src/crons",
            "HubletoProject\\Report\\": "src/reports",
            "HubletoProject\\Dependency\\": "src/dependencies"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "*",
        "phpunit/phpunit": "12",
        "fakerphp/faker": "^1",
        "dompdf/dompdf": "^3"
    }
}
